{
  "name": "moderntribe/core-plugin",
  "type": "project",
  "description": "Core functionality for this site.",
  "license": "Private",
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": "dist"
  },
  "require": {
    "pimple/pimple": "~3.0@dev",
    "johnbillion/extended-cpts": "^3.0",
    "johnbillion/extended-taxos": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tribe\\Project\\": "src/",
      "Tribe\\Libs\\": "vendor/moderntribe/tribe-libs/src/"
    },
    "files": [
      "vendor/johnbillion/extended-cpts/extended-cpts.php",
      "vendor/johnbillion/extended-taxos/extended-taxos.php"
    ]
  },
  "require-dev": {
    "lucatume/wp-browser": "^1.9",
    "lucatume/function-mocker": "~1.0"
  }
}
