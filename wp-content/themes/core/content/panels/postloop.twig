{# Panel: Post Loop #}

{% extends "content/panels/panel.twig" %}

{% block content %}

	<div class="s-content">
		<div class="g-row g-row--center g-row--col-4--min-full" {{ attrs }}>

            {% for post in posts %}
				<div class="t-content g-col">
                    {{ post }}
				</div>
            {% endfor %}
		</div>
	</div>

{% endblock %}
