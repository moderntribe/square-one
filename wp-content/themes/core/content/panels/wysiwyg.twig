{# Panel: WYSIWYG #}

{% extends "content/panels/panel.twig" %}

{% block content %}

	<div class="s-content">
		<div class="g-row">
			<div class="g-row--col-2--min-medium g-row--col-3--min-full"
			     data-depth="0"
			     data-name="columns"
			     data-livetext>

				{% for col in columns %}
					<div class="g-col t-content"
					     data-depth="{{ loop.index0 }}"
					     data-name="content"
					     data-autop="true"
					     data-livetext="true">
						 {{ col.content }}
					</div>
				{% endfor %}

			</div>
		</div>
	</div>

{% endblock %}
